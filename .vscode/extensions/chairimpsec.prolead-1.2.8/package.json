{
	"name": "prolead",
	"displayName": "PROLEAD",
	"description": "PROLEAD allows to analyze the robust probing security of masked implementations provided as a verilog netlist or arm binary",
	"version": "1.2.8",
	"engines": {
		"vscode": "^1.73.0"
	},
	"publisher": "ChairImpSec",
	"license": "BSD 3-Clause License",
	"homepage": "https://informatik.rub.de/impsec/",
	"repository": {
		"type": "git",
		"url": "https://github.com/ChairImpSec/PROLEAD"
	},
	"bugs": {
		"url": "https://github.com/ChairImpSec/PROLEAD/issues"
	},
	"icon": "icon.png",
	"categories": [
		"Debuggers",
		"Testing",
		"Programming Languages"
	],
	"keywords": [
		"c/c++",
		"ARM-processor",
		"side-channel attack",
		"masked implementation"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "prolead.prepare",
				"title": "Get Ready",
				"category": "PROLEAD",
				"icon": {
					"light": "media/prpr-L.svg",
					"dark": "media/prpr-D.svg"
				}
			},
			{
				"command": "prolead.installrqmts",
				"title": "Install Requirements",
				"category": "PROLEAD"
			},
			{
				"command": "prolead.evaluate",
				"title": "Evaluate",
				"category": "PROLEAD",
				"icon": {
					"light": "media/evlt-L.svg",
					"dark": "media/evlt-D.svg"
				}
			},
			{
				"command": "prolead.configure",
				"title": "Configure",
				"category": "PROLEAD",
				"icon": {
					"light": "media/cfgr-L.svg",
					"dark": "media/cfgr-D.svg"
				}
			},
			{
				"command": "prolead.createproject",
				"title": "Create Project",
				"category": "PROLEAD",
				"icon": {
					"light": "media/pjct-L.svg",
					"dark": "media/pjct-D.svg"
				}
			},
			{
				"command": "prolead.editlibrary",
				"title": "Edit Library",
				"category": "PROLEAD",
				"icon": {
					"light": "media/libr-L.svg",
					"dark": "media/libr-D.svg"
				}
			}
		],
		"keybindings": [
			{
				"command": "prolead.editlibrary",
				"key": "ctrl+alt+e"
			},
			{
				"command": "prolead.prepare",
				"key": "ctrl+alt+s"
			},
			{
				"command": "prolead.evaluate",
				"key": "ctrl+alt+f5"
			},
			{
				"command": "prolead.configure",
				"key": "ctrl+alt+c"
			},
			{
				"command": "prolead.createproject",
				"key": "ctrl+alt+p"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "prolead.createproject",
					"group": "navigation@1",
					"when": "config.prolead.runTime.editorIcons"
				},
				{
					"command": "prolead.editlibrary",
					"group": "navigation@2",
					"when": "config.prolead.runTime.editorIcons"
				},
				{
					"command": "prolead.configure",
					"group": "navigation@3",
					"when": "config.prolead.runTime.editorIcons"
				},
				{
					"command": "prolead.prepare",
					"group": "navigation@4",
					"when": "config.prolead.runTime.editorIcons"
				},
				{
					"command": "prolead.evaluate",
					"group": "navigation@5",
					"when": "config.prolead.runTime.editorIcons"
				}
			]
		},
		"walkthroughs": [
			{
				"id": "proleadwalkthrough",
				"title": "Getting started with PROLEAD",
				"description": "This tutorial will help you set up everything to start using PROLEAD.\nTo stop the automatic start of the walkthrough go to the extension settings.",
				"icon": "icon.svg",
				"steps": [
					{
						"id": "prolead.installdpncs",
						"title": "Requirements",
						"description": "Please make sure first to add the 'universe' repository by running:\n``sudo add-apt-repository universe``\n[Install All Packages](command:prolead.installdpncs)",
						"completionEvents": [
							"onCommand:prolead.installdpncs"
						],
						"media": {
							"markdown": "walkthroughs/requirements.md"
						}
					},
					{
						"id": "prolead.setcode",
						"title": "Get the Source Code",
						"description": "Choose to specify the path to your PROLEAD code or to clone the code to the standard path\n[Get Source Code](command:prolead.getcode)",
						"completionEvents": [
							"onSettingChanged:prolead.runTime.codeFolder"
						],
						"media": {
							"markdown": "walkthroughs/sourcecode.md"
						}
					},
					{
						"id": "prolead.createproject",
						"title": "Create a New Project",
						"description": "Start with PROLEAD and create a new project\n[Create Project](command:prolead.createproject)",
						"completionEvents": [
							"onCommand:prolead.createproject"
						],
						"media": {
							"markdown": "walkthroughs/newproject.md"
						}
					},
					{
						"id": "prolead.optionalprolead",
						"title": "Optional: Explore Extensions Settings",
						"description": "Check the extension settings at **Extensions>PROLEAD**\n[Show Settings](command:workbench.action.openSettings2)",
						"completionEvents": [
							"onCommand:workbench.action.openSettings2"
						],
						"media": {
							"image": "walkthroughs/media/PROLEAD-Settings.gif",
							"altText": "PROLEAD Settings"
						}
					},
					{
						"id": "prolead.optionalwindow",
						"title": "Optional: Change Popup Messages Style",
						"description": "Change popup messages style at **Window>Dialog Style**\n[Show Settings](command:workbench.action.openSettings2)",
						"completionEvents": [
							"onCommand:workbench.action.openSettings2"
						],
						"media": {
							"image": "walkthroughs/media/Window-Settings.gif",
							"altText": "Window Settings"
						}
					}
				]
			}
		],
		"configuration": [
			{
				"type": "object",
				"title": "PROLEAD",
				"properties": {
					"prolead.runTime.codeFolder": {
						"type": "string",
						"scope": "resource",
						"description": "Set the path to the folder where your PROLEAD code is"
					},
					"prolead.runTime.editorIcons": {
						"type": "boolean",
						"description": "Decide to show the icons of PROLEAD on top of the editor or not",
						"default": true
					},
					"prolead.runTime.timesActivated": {
						"type": "integer",
						"default": "0",
						"description": "Number of times the extension was activated",
						"readOnly": "true"
					},
					"prolead.runTime.welcomePage": {
						"type": "boolean",
						"description": "Show the welcome page on startup every time",
						"default": true
					},
					"prolead.statusBar.proleadIcons": {
						"type": "boolean",
						"description": "Show or hide PROLEAD icons in the status bar on startup",
						"default": true
					},
					"prolead.templates.configFile": {
						"type": "string",
						"scope": "resource",
						"description": "The path to the config file template to use when creating new prolead projects (leave empty to set default path)"
					},
					"prolead.templates.designFile": {
						"type": "string",
						"scope": "resource",
						"description": "The path to the design file template to use when creating new prolead projects (leave empty to set default path)"
					},
					"prolead.templates.libraryFile": {
						"type": "string",
						"scope": "resource",
						"description": "The path to the library file template to use when creating new prolead projects (leave empty to set default path)"
					},
					"prolead.templates.linkerFile": {
						"type": "string",
						"scope": "resource",
						"description": "The path to the linker file template to use when creating new prolead projects (leave empty to set default path)"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"@vscode/test-electron": "^2.2.0",
		"electron": "^23.2.0",
		"eslint": "^8.26.0",
		"glob": "^8.0.3",
		"mocha": "^10.1.0",
		"ts-loader": "^9.4.1",
		"typescript": "^4.8.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	},
	"__metadata": {
		"id": "7ed3ce6e-2835-43ac-94ae-9004c5032221",
		"publisherDisplayName": "ChairImpSec",
		"publisherId": "6ee89efd-81c4-4995-9e1b-07135666c214",
		"isPreReleaseVersion": false,
		"targetPlatform": "undefined",
		"isApplicationScoped": false,
		"updated": true,
		"installedTimestamp": 1691571454672,
		"preRelease": false
	}
}